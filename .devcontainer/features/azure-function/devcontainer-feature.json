{
  "id": "local.azure-function",
  "version": "1.0.0",
  "name": "Azure Function",
  "description": "Installs Azure Function Core Tools",
  "dependsOn": {
      "./features/common-deps": {},
      "ghcr.io/devcontainers/features/dotnet:2": {
        "version" : "8.0.300"
      }
  },
  "containerEnv": {
    "AZURE_FUNC_TOOLS_DIR": "/usr/local/functools",
    "PATH": "/usr/local/functools:${PATH}"
}
}
